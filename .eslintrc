{
    "extends": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "plugins": ["prettier"],
    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off",
        "no-process-exit": "off",
        "object-shorthand": "off",
        "class-methods-use-this": "off"
    }
}